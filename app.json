{
  "expo": {
    "name": "OnDVay_Expo",
    "slug": "OnDVay_Expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "mdpi": "./assets/android/drawable-mdpi/icon.png",
        "hdpi": "./assets/android/drawable-hdpi/icon.png",
        "xhdpi": "./assets/android/drawable-xhdpi/icon.png",
        "xxhdpi": "./assets/android/drawable-xxhdpi/icon.png",
        "xxxhdpi": "./assets/android/drawable-xxxhdpi/icon.png",
        "resizeMode": "cover"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
